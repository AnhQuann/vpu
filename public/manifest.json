{
  "manifest_version": 2,
  "name": "Vocabulary Push Up",
  "description": "Push up your vocabulary",
  "version": "1.0",
  "icons": {
    "128": "icons/icon128.png",
    "48": "icons/icon128.png",
    "16": "icons/icon128.png"
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "content_security_policy": "script-src 'self' 'sha256-x+1czqaozv46RtUyLm02eGVBvwc5GUAC6A95TsZXhrc='; object-src 'self'"
}